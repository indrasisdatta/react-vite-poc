{
  "name": "react-vite-poc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-linting-warnings": "eslint src --ext ts,tsx --max-warnings 0 --report-unused-disable-directives || echo 'Linting failed due to warnings.' && exit 1",
    "preview": "vite preview",
    "test": "set REACT_APP_TEST_ENV=true && vitest",
    "test-check": "set REACT_APP_TEST_ENV=true vitest --no-watch",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.0"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.4"
  }
}
